	--color-secondary-100: 250 249 244; /* ⬅ #faf9f4 */
	--color-secondary-200: 249 247 241; /* ⬅ #f9f7f1 */
	--color-secondary-300: 245 243 232; /* ⬅ #f5f3e8 */
	--color-secondary-400: 238 233 215; /* ⬅ #eee9d7 */
	--color-secondary-500: 231 224 198; /* ⬅ #e7e0c6 */
