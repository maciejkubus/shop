
:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: 'Noto Sans', sans-serif;
	--theme-font-family-heading: 'Poppins', sans-serif;;
	--theme-font-color-base: 0 0 0;
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 4px;
	--theme-rounded-container: 4px;
	--theme-border-base: 1px;
	--site-width: 1140px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: var(--color-tertiary-500);
	--on-secondary: var(--color-surface-500);
	--on-tertiary: var(--color-surface-500);
	--on-success: var(--color-surface-500);
	--on-warning: var(--color-surface-500);
	--on-error: var(--color-surface-500);
	--on-surface: var(--color-tertiary-500);
	/* =~= Theme Colors  =~= */
	/* primary | #3c4b9c */
	--color-primary-50: 226 228 240; /* ⬅ #e2e4f0 */
	--color-primary-100: 216 219 235; /* ⬅ #d8dbeb */
	--color-primary-200: 206 210 230; /* ⬅ #ced2e6 */
	--color-primary-300: 177 183 215; /* ⬅ #b1b7d7 */
	--color-primary-400: 119 129 186; /* ⬅ #7781ba */
	--color-primary-500: 60 75 156; /* ⬅ #3c4b9c */
	--color-primary-600: 54 68 140; /* ⬅ #36448c */
	--color-primary-700: 45 56 117; /* ⬅ #2d3875 */
	--color-primary-800: 36 45 94; /* ⬅ #242d5e */
	--color-primary-900: 29 37 76; /* ⬅ #1d254c */
	/* secondary | #e7e0c6 */
